{
    "name": "eloverde-sistemas/laravel-crud-builder",
    "homepage": "https://github.com/eloverde-sistemas/laravel-crud-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Filipe Trist√£o",
            "email": "filipedtristao@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3|^8.4",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "laravel/legacy-factories": "^1.4.1",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "CrudBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CrudBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}